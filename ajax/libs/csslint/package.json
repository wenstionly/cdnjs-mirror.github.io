{
  "author": "Nicole Sullivan",
  "description": "Automated linting of Cascading Stylesheets",
  "filename": "csslint.js",
  "homepage": "http://csslint.net/",
  "keywords": [
    "css",
    "styling",
    "lint",
    "linting"
  ],
  "license": "MIT",
  "name": "csslint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CSSLint/csslint.git"
  },
  "version": "1.0.5",
  "autoupdate": {
    "source": "npm",
    "target": "csslint",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*.js"
        ]
      }
    ]
  }
}
